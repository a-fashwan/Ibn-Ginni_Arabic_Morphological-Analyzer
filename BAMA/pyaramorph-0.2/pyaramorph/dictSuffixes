; triptote definite:
	ُ	NSuff-u	[def.nom.]   <pos>ُ/CASE_DEF_NOM</pos>
	َ	NSuff-a	[def.acc.]   <pos>َ/CASE_DEF_ACC</pos>
	ِ	NSuff-i	[def.gen.]   <pos>ِ/CASE_DEF_GEN</pos>
; triptote indefinite:
	ٌ	NSuff-N	[indef.nom.] <pos>ٌ/CASE_INDEF_NOM</pos>
	َ	NSuff-F	[indef.acc.] <pos>َ/CASE_INDEF_ACC</pos>
	ٍ	NSuff-K	[indef.gen.] <pos>ٍ/CASE_INDEF_GEN</pos>
; diptote indefinite:
; ma$AriyE+u jadiyd+ap+N, ma$AriyE+a jadiyd+ap+F, ma$AriyE+a jadiyd+ap+K
; EAlam+N >afoDal+u, EAlam+AF >afoDal+a, EAlam+K >afoDal+a
	ُ	NSuff-u_indef_nom	[indef.nom.]   <pos>ُ/CASE_INDEF_NOM</pos>
	َ	NSuff-a_indef_acc	[indef.acc.]   <pos>َ/CASE_INDEF_ACC</pos>
	َ	NSuff-a_indef_gen	[indef.gen.]   <pos>َ/CASE_INDEF_GEN</pos>
; for defective: qAD+K layAl+K
	ٍ	NSuff-K_nom	[indef.nom.] <pos>ٍ/CASE_INDEF_NOM</pos>
; for indeclinable: "NF_indec" EaSAF, mabonYF, musam~YF
	َ	NSuff-F_nom	[indef.nom.] <pos>َ/CASE_INDEF_NOM</pos>
َ	َ	NSuff-F_nom	[indef.nom.] <pos>َ/CASE_INDEF_NOM</pos>
;
	َ	NSuff-F_acc	[indef.acc.] <pos>َ/CASE_INDEF_ACC</pos>
َ	َ	NSuff-F_acc	[indef.acc.] <pos>َ/CASE_INDEF_ACC</pos>
;
	َ	NSuff-F_gen	[indef.gen.] <pos>َ/CASE_INDEF_GEN</pos>
َ	َ	NSuff-F_gen	[indef.gen.] <pos>َ/CASE_INDEF_GEN</pos>
;
ما	ما	NSuff-mA	what <pos>ما/REL_PEON</pos>
