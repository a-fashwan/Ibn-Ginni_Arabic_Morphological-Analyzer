; perfect verb, -a suffix
	َ	PVSuff-a	he/it                 <pos>َ/PVSUFF_SUBJ:3MS</pos>
;
; perfect verb, null suffix: banaY
		PVSuff-0a	he/it                 <pos>/PVSUFF_SUBJ:3MS</pos>
; perfect verb, null suffix: banA-h, daEA-h
ه	هُ	PVSuff-0ah	he/it + it/him          <pos>/PVSUFF_SUBJ:3MS+هُ/PRN:3MS</pos>
هما	هُما	PVSuff-0ah	he/it + them (both)     <pos>/PVSUFF_SUBJ:3MS+هُما/PRN:3D</pos>
